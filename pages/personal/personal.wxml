
<!--personal.wxml-->
<view class="personal">

  <!--用户-->
  <view class="user">
    <view>
      <image src="http://www.cxcycf.com/templates/main/images/user-avatar.png"></image>
    </view>
    <view>
      {{userInfo}}
    </view>
  </view>

  <!--资源管理-->
  <view class="span">
    <view><navigator class="arrow">我的收藏</navigator></view>
    <view><navigator class="arrow">用户足迹</navigator></view>
    <view><navigator class="arrow">我的文章</navigator></view>
    <view><navigator class="arrow">我的问答</navigator></view>
    <view><navigator class="arrow">我的课堂</navigator></view>
  </view>

  <!--项目管理-->
  <view class="span">
    <view><navigator class="arrow">我的项目</navigator></view>
    <view><navigator class="arrow">合伙人招募</navigator></view>
    <view><navigator class="arrow">项目通知</navigator></view>
    <view><navigator class="arrow">项目报表</navigator></view>
  </view>

  <!--个人信息-->
  <view class="span">
    <view><navigator class="arrow">个人资料</navigator></view>
    <view><navigator class="arrow">我的导师</navigator></view>
    <view><navigator class="arrow">我的招募申请</navigator></view>
    <view><navigator class="arrow">我的留言</navigator></view>
    <view><navigator class="arrow">退出登录</navigator></view>
  </view>
  <button open-type="getUserInfo"/>登录
</view>